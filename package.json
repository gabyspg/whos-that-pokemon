{
  "name": "whos-that-pokemon",
  "version": "1.0.0",
  "description": "",
  "main": "./client/index.js",
  "scripts": {
    "start": "nodemon server/server.js",
    "build": "NODE_ENV=production webpack",
    "dev": "concurrently \"nodemon server/server.js\" \"NODE_ENV=development webpack serve --open\"",
    "gulp-prod": "node_modules/.bin/gulp prod",
    "gulp-dev": "node_modules/.bin/gulp dev",
    "test": "jest --verbose"
  },
  "nodemonConfig": {
    "ignore": [
      "build",
      "client"
    ]
  },
  "jest": {
    "testEnvironment": "jest-environment-jsdom",
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  },
  "author": "conniedonahue https://github.com/*** <hello@codesmith.io>",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.23.0",
    "@reduxjs/toolkit": "^1.9.7",
    "babel-loader": "^9.1.3",
    "bcryptjs": "^2.4.3",
    "concurrently": "^8.2.1",
    "css-loader": "^6.8.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.50.0",
    "express": "^4.18.2",
    "html-webpack-plugin": "^5.5.3",
    "jest-environment-jsdom": "^29.7.0",
    "mongoose": "^7.5.3",
    "node-fetch": "^2.7.0",
    "nodemon": "^3.0.1",
    "prop-types": "^15.8.1",
    "puppeteer": "^21.3.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.16.0",
    "regenerator-runtime": "^0.14.0",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "babel-cli": "^6.26.0",
    "babel-jest": "^29.7.0",
    "babel-preset-env": "^1.7.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "react-test-renderer": "^18.2.0",
    "superagent": "^8.1.2",
    "supertest": "^6.3.3"
  }
}
